algoritmo "ex3"

var
peso, peso2: real
pesog, pesog2: real




inicio
escreva("Digite seu peso:")
leia(peso)

pesog <- peso*1000
escreval("Peso atual:",peso,"Kg ou",pesog,"g")

peso2 <- peso*1.12
pesog2 <- peso2*1000

escreval("Peso após engordar 12% do peso inicial:",peso2,"Kg ou",pesog2,"g")



fimalgoritmo
