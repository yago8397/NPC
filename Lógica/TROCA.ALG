Algoritmo "troca"
Var
num1, num2, aux: real


Inicio
escreval("Digite num1.")
leia(num1)

escreva("Digite num2.")
leia(num2)

escreval("Valor de Entrada",num1)
escreval("Valor de Entrada",num2)
aux <- num1
num1 <- num2
num2 <- aux

escreval("Valor após a troca",num1)
escreval("Valor após a troca", num2)

Fimalgoritmo