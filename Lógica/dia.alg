algoritmo "dias da semana"

var
dia: inteiro

inicio
escreva("Digite um número:")
leia(dia)

escolha(dia)

caso 0
escreva("Domingo")

caso 1
escreva("Segunda")

caso 2
escreva("Terça")

caso 3
escreva("Quarta")

caso 4
escreva("Quinta")

caso 5
escreva("Sexta")

caso 6
escreva("Sábado")

outrocaso
escreva("Dado Inválido")




fimescolha
fimalgoritmo
